<nav aria-label="breadcrumb">
  <ol class="breadcrumb">
    <li class="breadcrumb-item">{{#link-to 'index'}}<a href="#">Home</a>{{/link-to}}</li>
    <li class="breadcrumb-item"><a href="#">Anggota</a></li>
    <li class="breadcrumb-item active" aria-current="page">Form</li>
  </ol>
</nav>

<form class="form-narrow form-horizontal"   >
	<fieldset>
        <div class="form-group form-group-sm">
            <label for="Nama" 
                class="col-xs-12 col-sm-3 col-md-3 control-label">Nama</label>
            <div class="col-xs-12 col-sm-9 col-md-8">
                {{input value=model.nama class="form-control" placeholder="Nama"}}
            </div>
        </div>
        <div class="form-group form-group-sm">
            <label for="KotaLahir" 
                class="col-xs-12 col-sm-3 col-md-3 control-label">Kota Lahir</label>
            <div class="col-xs-12 col-sm-9 col-md-8">
                {{input value=model.kota_lahir class="form-control" placeholder="Kota Lahir"}}
            </div>
        </div>
        <div class="form-group form-group-sm">
            <label for="tanggalLahir" 
                class="col-xs-12 col-sm-3 col-md-3 control-label">Tanggal Lahir </label>
                <div class="col-xs-12 col-sm-9 col-md-8">
                    <div id="datePickerInputGroup" class="input-group date" > {{input value=model.tanggal_lahir class="form-control" placeholder="Tanggl Lahir"}}
                        <span class="input-group-addon">
                            <span class="glyphicon glyphicon-calendar"></span>
                        </span> 
                    </div>
                    {{!-- {{pikaday-input placeholder="Due date of invoice"}} --}}
                </div>
        </div>
        <div class="form-group form-group-sm">
            <label for="alamat" 
                class="col-xs-12 col-sm-3 col-md-3 control-label">Alamat</label>
            <div class="col-xs-12 col-sm-9 col-md-8">
                {{input value=model.alamat class="form-control" placeholder="Alamat"}}
            </div>
        </div>
        <div class="form-group form-group-sm">
            <label for="kontak" class="col-xs-12 col-sm-3 col-md-3 control-label">Kontak</label>
            <div class="col-xs-12 col-sm-9 col-md-8">
                {{input value=model.kontak class="form-control" placeholder="Kontak"}}
            </div>
        </div>
        <div class="form-group form-group-sm">
            <label for="email" 
                class="col-xs-12 col-sm-3 col-md-3 control-label">Email</label>
            <div class="col-xs-12 col-sm-9 col-md-8">
                 {{input value=model.email class="form-control" placeholder="Email"}}
            </div>
        </div>
        <div class="form-group form-group-sm">
            <label for="prodi" 
                    class="col-xs-12 col-sm-3 col-md-3 control-label">Prodi</label>
            <div class="col-xs-12 col-sm-9 col-md-8">
                 {{input value=model.prodi class="form-control" placeholder="Program Studi"}}
            </div>
        </div>
        <div class="form-group form-group-sm">
            <label for="universitas" 
                class="col-xs-12 col-sm-3 col-md-3 control-label">Universitas</label>
            <div class="col-xs-12 col-sm-9 col-md-8">
                {{input value=model.universitas class="form-control" placeholder="Universitas"}}
            </div>
        </div>
        <div class="form-group form-group-sm">
            <label for="angkatan" 
                class="col-xs-12 col-sm-3 col-md-3 control-label">Angkatan</label>
            <div class="col-xs-12 col-sm-9 col-md-8">
                {{input value=model.angkatan class="form-control" placeholder="Angkatan"}}
            </div>
        </div>
        <div class="form-group form-group-sm">
            <label for="keanggotaan"
            class="col-xs-12 col-sm-3 col-md-3 control-label">Keanggotaan</label>
            <div class="col-xs-12 col-sm-9 col-md-8">
                 {{input value=model.keanggotaan class="form-control" placeholder="Keanggotaan"}}
            </div>
        </div>
        {{!-- <div class="form-group form-group-sm">
            <label for="pic"
            class="col-xs-12 col-sm-3 col-md-3 control-label">Foto</label>
            <div class="col-xs-12 col-sm-9 col-md-8">
                 {{input value=model.pic class="form-control" placeholder="Foto"}}
            </div>
        </div> --}}
        <div class="form-group form-group-sm">
            <div class="col-sm-offset-3 col-md-offset-3 col-xs-12 col-sm-9 col-md-8">
                <button type="submit" class="btn btn-primary btn-sm" {{action 'createAnggota' model}}><span class="glyphicon glyphicon-ok" aria-hidden="true">&nbsp;Simpan</span></button>
                {{#link-to 'anggota.list'}}<a href="#"  class="btn btn-default btn-sm"><span class="glyphicon glyphicon-remove" aria-hidden="true">&nbsp;Tunda</span></a>{{/link-to}}
            </div>
        </div>
    </fieldset>
</form>


